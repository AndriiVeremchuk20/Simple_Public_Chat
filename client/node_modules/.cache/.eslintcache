xableWarningCount":0,"source":null},{"filePath":"169","messages":"170","suppressedMessages":"171","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"172","usedDeprecatedRules":"173"},"/home/andrei/Code/Twitter_clon/client/src/App.tsx",["174","175"],[],"/home/andrei/Code/Twitter_clon/client/src/pages/WaitPage.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/pages/Home.tsx",["176"],[],"/home/andrei/Code/Twitter_clon/client/src/routes.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/pages/Login.tsx",["177","178"],[],"import React, { useCallback, useState } from \"react\";\nimport {\n  Alert,\n  Box,\n  Button,\n  CircularProgress,\n  Paper,\n  TextField,\n} from \"@mui/material\";\nimport { Controller, SubmitHandler, useForm } from \"react-hook-form\";\nimport { useMutation } from \"react-query\";\nimport { yupResolver } from \"@hookform/resolvers/yup\";\nimport * as Yup from \"yup\";\n\nimport { AppServises } from \"../servises/API\";\nimport { LoginUser, User } from \"../types/User\";\nimport { Link, useNavigate } from \"react-router-dom\";\nimport { AppRoutes } from \"../routes\";\nimport { Token } from \"../utils/token\";\nimport { useAtom } from \"jotai\";\nimport { appUserAtom } from \"../atom\";\n\nexport const Login = () => {\n  const [showPassword, setShowPassword] = useState<boolean>(false);\n  const [errorMessage, setErrorMessage] = useState<string>(\"\");\n  const [,setUser] = useAtom(appUserAtom);\n  const navigate = useNavigate();\n\n  const formScheme = Yup.object().shape({\n    password: Yup.string()\n      .required(\"password is required\")\n      .min(4, \"Min password lenght 4\")\n      .max(12, \"Max password lenght 4\"),\n    username: Yup.string()\n      .required(\"username is reqiured\")\n      .min(4, \"Min length for username 4\"),\n  });\n\n  const {\n    handleSubmit,\n    control,\n    //formState: { errors },\n  } = useForm<LoginUser>({\n    mode: \"onChange\",\n    resolver: yupResolver(formScheme),\n  });\n\n  const { mutate, isLoading, isError } = useMutation(\n    AppServises.login,\n    {\n      onSuccess: (data) => {\n        setUser(data.user);\n        navigate(0);\n      },\n      onError: (error: any) => {\n        const errorText = error.response.data.msg\n          ? error.response.data.msg\n          : error.message;\n        setErrorMessage(errorText);\n\n        Token.Remove();\n      },\n    }\n  );\n\n  const onSubmit: SubmitHandler<LoginUser> = useCallback((data) => {\n    const user = {\n      ...data,\n    };\n    mutate(user);\n  }, []);\n\n  const onButtonClick = useCallback(() => {\n    setShowPassword((prev) => !prev);\n  }, []);\n\n  return (\n    <Paper sx={styles.main}>\n      <Box sx={styles.form}>\n        <Controller\n          name=\"username\"\n          control={control}\n          defaultValue=\"\"\n          render={({ field: { onChange, value }, fieldState: { error } }) => (\n            <TextField\n              sx={styles.input}\n              error={!(error == null)}\n              required\n              type={\"text\"}\n              label=\"Username\"\n              onChange={onChange}\n              value={value}\n              variant=\"outlined\"\n              helperText={error != null ? error.message : null}\n            />\n          )}\n        />\n\n        <Box sx={styles.inputPassword}>\n          <Controller\n            name=\"password\"\n            control={control}\n            defaultValue=\"\"\n            render={({ field: { onChange, value }, fieldState: { error } }) => (\n              <TextField\n                required\n                error={!(error == null)}\n                type={showPassword ? \"text\" : \"password\"}\n                label=\"Password\"\n                onChange={onChange}\n                value={value}\n                variant=\"outlined\"\n                helperText={error != null ? error.message : null}\n              />\n            )}\n          />\n          <Button onClick={onButtonClick}>{showPassword ? \"🫣\" : \"😶‍🌫️\"}</Button>\n        </Box>\n        <Button sx={styles.input} onClick={handleSubmit(onSubmit)}>\n          {isLoading ? <CircularProgress size={30} /> : \"Login\"}\n        </Button>\n\n        {isError ? <Alert color=\"error\">{errorMessage}</Alert> : null}\n        <Link to={AppRoutes.registration}>Registration</Link>\n      </Box>\n    </Paper>\n  );\n};\n\nconst styles = {\n  main: {\n    height: \"100vh\",\n    display: \"flex\",\n    justifyContent: \"center\",\n    alignItems: \"center\",\n  },\n  form: {\n    display: \"flex\",\n    flexDirection: \"column\",\n    justifyContent: \"center\",\n    alignItems: \"center\",\n    border: \"solid black 3px\",\n    borderRadius: \"5px\",\n    padding: \"20px\",\n  },\n  input: {\n    width: \"100%\",\n    margin: \"5px\",\n  },\n  inputPassword: {\n    margin: \"10px 0 0 0\",\n    display: \"flex\",\n    justifyContent: \"center\",\n    alignItems: \"center\",\n  },\n};\n",[],"/home/andrei/Code/Twitter_clon/client/src/components/SearchBar.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/Logo.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/Search.tsx",["179"],[],"/home/andrei/Code/Twitter_clon/client/src/components/UserInfo.tsx",["180","181","182"],[],"/home/andrei/Code/Twitter_clon/client/src/servises/API.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/pages/Registration.tsx",["183"],[],"/home/andrei/Code/Twitter_clon/client/src/components/UserAutoCompleteCard.tsx",["184"],[],"/home/andrei/Code/Twitter_clon/client/src/hooks/useDebounce.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/pages/Profile.tsx",["185","186"],[],"/home/andrei/Code/Twitter_clon/client/src/pages/NoPage.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/MakePost.tsx",["187","188","189"],[],"/home/andrei/Code/Twitter_clon/client/src/atom/index.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/PostsList.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/PostCard.tsx",["190","191","192"],[],"/home/andrei/Code/Twitter_clon/client/src/components/UpButton.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/utils/parseDate.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/index.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/utils/token.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/ChangeTheme.tsx",["193"],[],"/home/andrei/Code/Twitter_clon/client/src/components/Footer.tsx",[],[],"/home/andrei/Code/Twitter_clon/client/src/types/Post.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/utils/likesInfo.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/types/User.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/utils/chekSubscribed.ts",[],[],"/home/andrei/Code/Twitter_clon/client/src/components/UsersList.tsx",["194"],[],"/home/andrei/Code/Twitter_clon/client/src/components/UserCard.tsx",["195","196"],[],"import {\n  Avatar,\n  Box,\n  Divider,\n  ListItem,\n  ListItemAvatar,\n  ListItemButton,\n  ListItemText,\n  Typography,\n} from \"@mui/material\";\nimport React, { useCallback } from \"react\";\nimport { useNavigate } from \"react-router-dom\";\nimport { User } from \"../types/User\";\nimport { SubscribeButton } from \"./SubscribeButton\";\n\ninterface UserCatdProps {\n  user: User;\n}\n\nexport const UserCard: React.FC<UserCatdProps> = ({ user }) => {\n  const navigate = useNavigate();\n\n  const onCardClick = useCallback(() => {\n    navigate(`/profile/${user._id}`, { replace: true });\n  }, []);\n\n  return (\n    <>\n      <ListItem\n        sx={{\n          \"&:hover\": {\n            background: \"#8a8a8a\",\n          },\n          width: \"100%\",\n          display: \"flex\",\n          flexDirection: \"row\",\n          justifyContent: \"space-between\",\n        }}\n      >\n        <Box\n          sx={{ width: \"40%\", display: \"flex\", flexDirection: \"row\" }}\n          onClick={onCardClick}\n        >\n          <ListItemAvatar>\n            <Avatar\n              alt={user.username}\n              src={user?.avatarUrl ?? \"\"}\n              sx={{ width: \"56\", height: \"56\", margin: \"0 10px 0 10px\" }}\n            />\n          </ListItemAvatar>\n\n          <ListItemText>\n            <Typography variant=\"h4\">{user.username}</Typography>\n          </ListItemText>\n        </Box>\n\n        <Box>\n            <SubscribeButton subscribeToUserId={user._id} />\n         \n        </Box>\n      </ListItem>\n\n      <Divider variant=\"inset\" component=\"li\" />\n    </>\n  );\n};\n",[],"/home/andrei/Code/Twitter_clon/client/src/components/SubscribeButton.tsx",["197","198"],[],"/home/andrei/Code/Twitter_clon/client/src/components/ProfileUserCard.tsx",["199"],[],"import {\n  Avatar,\n  Box,\n  Card,\n  CardActions,\n  CardContent,\n  CardMedia,\n  Drawer,\n  Typography,\n} from \"@mui/material\";\nimport React, { useCallback, useEffect, useState } from \"react\";\nimport { useMutation } from \"react-query\";\nimport { AppServises } from \"../servises/API\";\nimport { Post } from \"../types/Post\";\nimport { getSubscribedUsersResponse, Subscribe } from \"../types/Subscribe\";\nimport { User } from \"../types/User\";\nimport { SubscribeButton } from \"./SubscribeButton\";\nimport { UsersList } from \"./UsersList\";\n\ninterface ProfileUserCardProps {\n  user: User;\n  subscribers: Array<Subscribe>;\n  subscriptions: Array<Subscribe>;\n  posts: Array<Post>;\n}\n\nexport const ProfileUserCard: React.FC<ProfileUserCardProps> = ({\n  user,\n  subscribers,\n  subscriptions,\n  posts,\n}) => {\n  const [subscriptionsUsers, setSubscriptionsUsers] =\n    useState<getSubscribedUsersResponse | null>(null);\n  const [usersToShow, setUsersToShow] = useState<Array<User>>([]);\n  const [showUsers, setShowUsers] = useState<boolean>(false);\n\n  const getSubscribedUsers = useMutation(AppServises.getSubscribedUsers, {\n    onSuccess: (data) => {\n      console.log(data);\n    